<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <!-- The identifier that must be unique within the hosting gallery -->
        <id>upstox-dotnet-sdk</id>
        <title>Upstox DotNet SDK</title>

        <!-- The package version number that is used when resolving dependencies -->
        <version>1.2.0</version>

        <!-- Authors contain text that appears directly on the gallery -->
        <authors>Upstox</authors>

        <!-- Owners are typically nuget.org identities that allow gallery
             users to earily find other packages by the same owners.  -->
        <owners>Upstox</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <developmentDependency>false</developmentDependency>

        <!-- The description can be used in package manager UI. Note that the
             nuget.org gallery uses information you add in the portal. -->
        <description>The official .Net client for communicating with the Upstox API</description>

        <!-- Dependencies are automatically installed when the package is installed -->
        <dependencies>

            <dependency id="NewtonSoft.Json" version="13.0.1" />
            <dependency id="JsonSubTypes" version="1.2.0" />
            <dependency id="RestSharp" version="105.1.0" />
            <dependency id="Google.Protobuf" version="3.6.1" />
        </dependencies>

        <projectUrl>https://github.com/upstox/upstox-dotnet</projectUrl>
        <license type="expression">MIT</license>
        <tags>upstox trade sdk rest api csharp dotnet</tags>

    </metadata>
    <files>

        <!-- A readme.txt will be displayed when the package is installed -->
        <file src="..\..\README.md" target="" />
        <file src="..\..\docs\**\*.*" target="docs" />

    </files>
</package>
